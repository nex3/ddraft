{
  "name": "ddraft",
  "description": "Long-term draft simulator",
  "author": "Natalie Weizenbaum",
  "type": "module",
  "scripts": {
    "build": "run-s 'build:**'",
    "serve": "run-p 'watch:**'",
    "lint": "gts lint",
    "clean": "gts clean",
    "fix": "gts fix",
    "build:tsc": "tsc",
    "build:sass": "sass src/public/style.scss build/src/public/style.css",
    "watch:sass": "sass --watch src/public/style.scss build/src/public/style.css",
    "watch:express": "ts-node-esm src/index.ts"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.196",
    "body-parser": "^1.20.2",
    "byte-base64": "^1.1.0",
    "csv-parse": "^5.4.0",
    "express": "^4.18.2",
    "got": "^13.0.0",
    "gts": "^3.1.1",
    "liquidjs": "^10.8.4",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "pkg-dir": "^7.0.0",
    "sass": "^1.64.1",
    "sharp": "^0.32.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "varint": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/varint": "^6.0.1",
    "gts": "^3.1.1",
    "typescript": "~5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
